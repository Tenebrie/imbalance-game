{
	"name": "server",
	"version": "0.0.0",
	"license": "GPL-3.0-or-later",
	"private": true,
	"scripts": {
		"build": "tsc && mkdir -p dist/server/src/views && cp src/views/*.jade dist/server/src/views",
		"start": "node dist/server/src/app.js",
		"serve": "yarn nodemon --legacy-watch --watch 'src' --watch '../shared/src' -e 'ts,jade' --exec 'yarn serve-exec'",
		"serve-exec": "yarn tsc --incremental --tsBuildInfoFile dist/tsBuildInfo.json && mkdir -p dist/server/src/views && cp src/views/*.jade dist/server/src/views && node dist/server/src/app.js",
		"watch": "babel --watch src --out-dir dist --extensions .ts,.tsx",
		"lint": "yarn eslint src/ --ext .ts",
		"lint-fix": "yarn eslint src/ --ext .ts --fix",
		"test": "yarn jest"
	},
	"dependencies": {
		"@babel/cli": "^7.11.6",
		"@babel/core": "^7.11.6",
		"@babel/plugin-proposal-class-properties": "^7.10.4",
		"@babel/plugin-proposal-object-rest-spread": "^7.11.0",
		"@babel/plugin-transform-runtime": "^7.11.5",
		"@babel/preset-env": "^7.11.5",
		"@babel/preset-flow": "^7.10.4",
		"@babel/preset-typescript": "^7.10.4",
		"@babel/runtime": "^7.11.2",
		"@types/bcrypt": "^3.0.0",
		"@types/console-stamp": "^0.2.33",
		"@types/cookie-parser": "^1.4.2",
		"@types/express": "^4.17.8",
		"@types/express-session": "^1.17.0",
		"@types/express-ws": "^3.0.0",
		"@types/glob": "^7.1.3",
		"@types/jest": "^26.0.16",
		"@types/jsonwebtoken": "^8.5.0",
		"@types/morgan": "^1.9.1",
		"@types/pg": "^7.14.4",
		"@types/seedrandom": "^2.4.28",
		"@types/sequelize": "^4.28.9",
		"@types/uuid": "^8.3.0",
		"@typescript-eslint/eslint-plugin": "^4.7.0",
		"@typescript-eslint/parser": "^4.7.0",
		"acorn": "^8.0.4",
		"babel-eslint": "^10.1.0",
		"babel-node": "0.0.1-security",
		"babel-plugin-module-resolver": "^4.0.0",
		"babel-polyfill": "^6.26.0",
		"babel-preset-es2015": "^6.24.1",
		"babel-watch": "^7.0.0",
		"bcrypt": "^5.0.0",
		"canvas": "^2.6.1",
		"console-stamp": "^0.2.9",
		"cookie-parser": "^1.4.5",
		"copyfiles": "^2.3.0",
		"cors": "^2.8.5",
		"db-migrate": "^0.11.11",
		"debug": "~4.2.0",
		"eslint": "^7.16.0",
		"eslint-config-prettier": "^7.1.0",
		"eslint-plugin-flowtype": "^5.2.0",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-prettier": "^3.3.0",
		"express": "~4.17.1",
		"express-session": "^1.17.1",
		"express-ws": "^4.0.0",
		"flow-bin": "^0.138.0",
		"glob": "^7.1.6",
		"google-auth-library": "^6.1.3",
		"http-errors": "~1.8.0",
		"jade": "~1.11.0",
		"jest": "^26.6.3",
		"jsonwebtoken": "^8.5.1",
		"module-alias": "^2.2.2",
		"morgan": "~1.10.0",
		"njstrace": "^1.2.0",
		"node-pg-migrate": "^5.9.0",
		"node-uuid": "^1.4.8",
		"nodemon": "^2.0.4",
		"pg": "^8.5.1",
		"pgmigrate": "^5.0.0",
		"prettier": "^2.2.1",
		"prettier-eslint": "^12.0.0",
		"rgb-hex": "^3.0.0",
		"seedrandom": "^3.0.5",
		"sequelize": "^6.3.5",
		"ts-jest": "^26.4.4",
		"ts-node": "^9.0.0",
		"tspath": "^1.3.7",
		"typescript": "^4.0.5",
		"uuid": "^8.3.1"
	},
	"_moduleAliases": {
		"@src": "src",
		"@shared": "dist/shared/src"
	},
	"babel": {
		"presets": [
			"@babel/preset-env",
			"@babel/preset-typescript"
		],
		"plugins": [
			"@babel/proposal-class-properties",
			"@babel/proposal-object-rest-spread",
			"@babel/plugin-transform-runtime",
			[
				"module-resolver",
				{
					"alias": {
						"@src": "./src",
						"@shared": "../shared/src"
					}
				}
			]
		]
	}
}
